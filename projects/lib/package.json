{
  "name": "@elemental-concept/env-bakery",
  "version": "0.2.0",
  "description": "Loads environment variables from .env file into Angular environment",
  "license": "BSD-2-Clause",
  "private": false,
  "peerDependencies": {
    "@angular/common": ">= 12 < 17",
    "@angular/core": ">= 12 < 17"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elementalconcept/env-bakery.git"
  },
  "keywords": [
    "dotenv",
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings",
    "angular",
    "ng"
  ],
  "bin": {
    "parbake": "assets/bin/parbake.js"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "dependencies"
  },
  "sideEffects": false
}
